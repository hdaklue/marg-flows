{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "asmit/resized-column": "^1.1",
        "bumpcore/editor.php": "^1.2",
        "filament/filament": "^4.0",
        "hdaklue/actioncrumb": "^1.0",
        "hdaklue/larapath": "^2.0",
        "hdaklue/laravel-dto-morph-cast": "^1.1",
        "hdaklue/marg-brac": "^2.0",
        "hdaklue/naptab": "^1.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.34",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/volt": "^1.7.0",
        "lorisleiva/laravel-actions": "^2.9",
        "pbmedia/laravel-ffmpeg": "^8.7",
        "predis/predis": "^3.0",
        "propaganistas/laravel-disposable-email": "^2.4",
        "rinvex/countries": "^9.0",
        "spatie/eloquent-sortable": "^4.5",
        "spatie/image": "^3.8",
        "spatie/laravel-image-optimizer": "^1.8",
        "wendelladriel/laravel-validated-dto": "^4.1",
        "wire-elements/modal": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "laradumps/laradumps": "^4.5",
        "laradumps/laradumps-core": "^3.2",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^1.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.22",
        "laravel/sail": "^1.41",
        "laravel/telescope": "^5.9",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "peckphp/peck": "^0.1.3",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/date.php",
            "app/Helpers/filament.php",
            "app/Helpers/storage.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpstan:analyze": [
            "@php artisan cache:clear",
            "clear && php -d memory_limit=2G ./vendor/bin/phpstan analyse"
        ],
        "ds:check": "@php vendor/bin/laradumps check --dir=app,routes,config",
        "lint": [
            "mago format"
        ],
        "analyze": [
            "mago lint",
            "mago analyze"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hdaklue/marg-brac.git"
        },
        {
            "type": "package",
            "package": {
                "name": "hdaklue/marg-brac",
                "version": "2.0.5",
                "source": {
                    "url": "https://github.com/hdaklue/marg-brac.git",
                    "type": "git",
                    "reference": "2.0.5"
                },
                "autoload": {
                    "psr-4": {
                        "Hdaklue\\MargBrac\\": "src/"
                    }
                }
            }
        }
    ]
}
